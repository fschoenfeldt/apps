{
    "compileOnSave": false,
    "compilerOptions": {
        "jsx": "react",
        "lib": ["dom", "dom.iterable", "es2015", "es2016", "es2019"],
        "allowSyntheticDefaultImports": true,
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "helpers": ["./src/helpers"],
            "helpers/*": ["./src/helpers/*"],
            "actions": ["./src/actions"],
            "actions/*": ["./src/actions/*"],
            "apis": ["./src/apis"],
            "apis/*": ["./src/apis/*"]
        }
    },
    "include": ["kiebitz", "src", "src-vite", "custom.d.ts"]
}
