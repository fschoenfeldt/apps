    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.19.0",
    "typescript": "4.3.2",
    "url-loader": "3.0.0",
    "vite": "^2.3.7",
    "webpack": "5.38.1",
    "webpack-bundle-size-analyzer": "3.1.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "2.25.0",
    "yaml-loader": "0.5.0"
  },
  "dependencies": {
    "ts-loader": "^9.2.3"
  }
