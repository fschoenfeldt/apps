{
    "name": "kiebitz",
    "private": true,
    "version": "0.0.1",
    "description": "Kiebitz Web Applications",
    "license": "AGPL-3.0-or-later",
    "author": "Andreas Dewes",
    "scripts": {
        "prettier": "prettier --write 'src/**/*.jsx' 'src/**/*.tsx' 'src/**/*.js'",
        "analyze": "APP_ENV=production webpack --config webpack.config.js --json | webpack-bundle-size-analyzer",
        "lint-scss": "stylelint ./src/scss/*.scss --quiet",
        "lint-scss-fix": "stylelint ./src/scss/*.scss --fix",
        "lint": "eslint . --ext js,jsx,ts,tsx --quiet && stylelint src/**/*.scss",
        "lint-fix": "eslint . --ext js,jsx,ts,tsx --quiet --fix; stylelint src/**/*.scss --fix",
        "make-test": "APP_ENV=test webpack-dev-server --config webpack.config.js --history-api-fallback",
        "make-dev": "APP_ENV=dev webpack-dev-server --config webpack.config.js --history-api-fallback",
        "make": "webpack --config webpack.config.js",
        "i18n": "lingui",
        "i18n:extract": "lingui extract",
        "i18n:compile": "lingui compile"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-proposal-class-static-block": "^7.16.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
        "@babel/plugin-transform-async-to-generator": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-react": "7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@lingui/cli": "^3.13.0",
        "@lingui/macro": "^3.13.0",
        "@lingui/react": "^3.13.0",
        "@types/react": "^17.0.37",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "babel-loader": "^8.2.3",
        "babel-plugin-macros": "^3.1.0",
        "buffer": "^6.0.3",
        "copy-webpack-plugin": "^10.0.0",
        "css-loader": "^6.5.1",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "file-loader": "^6.2.0",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^2.4.3",
        "node-sass": "^6.0.1",
        "postcss-scss": "^4.0.2",
        "prettier": "^2.5.0",
        "process": "^0.11.10",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.20.5",
        "react-router": "^6.0.2",
        "react-router-dom": "^6.0.2",
        "react-use": "^17.3.1",
        "sass-loader": "^12.3.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.1.0",
        "stylelint-a11y": "^1.2.3",
        "stylelint-config-recommended": "^6.0.0",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "stylelint-no-unsupported-browser-features": "^5.0.2",
        "stylelint-scss": "^4.0.0",
        "typescript": "^4.5.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.64.4",
        "webpack-bundle-size-analyzer": "3.1.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0",
        "webpack-hot-middleware": "^2.25.1",
        "yaml-loader": "^0.6.0"
    },
    "browserslist": [
        "defaults",
        "not ie 11",
        "not op_mini all"
    ]
}
